{
  "name": "hunt_train",
  "version": "1.0.0",
  "main": "src/index.jsx",
  "scripts": {
    "test": "jest --coverage",
    "build:dev": "webpack-dev-server --env dev",
    "build:prod": "./node_modules/.bin/webpack --env prod --config webpack.prod.js",
    "lint": "eslint src -c .eslintrc.json --ext js,jsx"
  },
  "author": "Keith Alleman",
  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.5.5",
    "@testing-library/jest-dom": "^4.0.0",
    "axios": "^0.19.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-runtime": "^6.23.0",
    "dotenv-webpack": "^1.7.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "immer": "^3.2.0",
    "jest-styled-components": "^6.3.3",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "styled-components": "^4.3.2",
    "use-immer": "^0.3.3",
    "webpack": "^4.36.1",
    "webpack-dev-server": "^3.7.2"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.5.5",
    "babel-jest": "^24.8.0",
    "css-loader": "^3.1.0",
    "enzyme-to-json": "^3.3.5",
    "eslint": "5.16",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-import-resolver-webpack": "^0.11.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "jest": "^24.8.0",
    "jest-haste-map": "^24.8.1",
    "node-sass": "^4.12.0",
    "prettier": "^1.18.2",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "webpack-cli": "^3.3.6"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>src/setupTests.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "moduleNameMapper": {
      "@A(.*)$": "<rootDir>/src/components/_atoms/$1",
      "@M(.*)$": "<rootDir>/src/components/_molecules/$1",
      "@O(.*)$": "<rootDir>/src/components/_organisms/$1"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint"
    ],
    "*.{js,jsx,json,md}": [
      "prettier --list-different"
    ]
  }
}
